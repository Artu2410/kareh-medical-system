# Base de datos PostgreSQL (requerida)
DATABASE_URL="postgresql://user:password@localhost:5432/kareh_medical?schema=public"

# JWT Secret para autenticación
JWT_SECRET="your-super-secret-jwt-key-change-in-production"

# Puerto de desarrollo
PORT=4000

# Entorno
NODE_ENV="development"

# CORS - URLs permitidas
CORS_ORIGIN="http://localhost:5173"

# Máximo de citas por slot (recomendado: 5)
MAX_APPOINTMENTS_PER_SLOT=5

# Máximo de sesiones en un paquete (recomendado: 10)
PACKAGE_SIZE=10
